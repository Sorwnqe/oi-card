    "tsconfig": "workspace:*",
    "tslib": "^2.5.0",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "typescript": "^5.0.4"
  },
  "packageManager": "pnpm@7.28.0"
